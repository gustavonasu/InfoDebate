<%= render 'shared/admin/status_change/fixed_alert' %>
<%= render 'shared/admin/status_change/delete_case' %>

<% unless request[:notice].nil? %>
  <% if params[:call_from] == "comment_list" %>
    $("#comment_<%= @obj.id %>").addClass("item_changed");
    $("#comment_<%= @obj.id %>").fadeOut(1700);
    setTimeout('reloadPage()',1700);
  <% else %>
    <%= render 'shared/admin/status_change/reload_page' %>
  <% end %>
<% end %>
