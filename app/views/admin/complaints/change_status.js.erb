<%= render 'shared/admin/status_change/fixed_alert' %>
<%= render 'shared/admin/status_change/delete_case' %>

<% unless request[:notice].nil? %>
  <% if params[:call_from] == "comment_list" %>
    $("#comment_<%= @obj.comment.id %>").addClass("item_changed");
    $("#comment_<%= @obj.comment.id %>").fadeOut(2000);
    setTimeout('reloadPage()', 2000);
  <% elsif params[:call_from] == "complaint_list" %>
    $("#complaint_<%= @obj.id %>").addClass("item_changed");
    $("#complaint_<%= @obj.id %>").fadeOut(2000);
    setTimeout('reloadPage()', 2000);
  <% else %>
    <%= render 'shared/admin/status_change/reload_page' %>
  <% end %>
<% end %>
